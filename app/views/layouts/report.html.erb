<script src="http://pages.revox.io/dashboard/4.1.0/html/casual/assets/plugins/jquery-actual/jquery.actual.min.js"></script>
<script src="http://pages.revox.io/dashboard/4.1.0/html/casual/assets/plugins/jquery-scrollbar/jquery.scrollbar.min.js"></script>
<script type="text/javascript" src="http://pages.revox.io/dashboard/4.1.0/html/casual/assets/plugins/select2/js/select2.full.min.js"></script>
<script type="text/javascript" src="http://pages.revox.io/dashboard/4.1.0/html/casual/assets/plugins/classie/classie.js"></script>
